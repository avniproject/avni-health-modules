{
  "name": "avni-health-modules",
  "version": "0.0.10",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/avniproject/avni-health-modules.git"
  },
  "main": "dist/index.js",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "babel src --out-dir dist --extensions '.js' --ignore src/test",
    "test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/JestLifecycle"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testMatch": [
      "**/test/**/*Test.js"
    ],
    "verbose": false,
    "bail": true,
    "moduleNameMapper": {
      "^avni-models$": "<rootDir>/node_modules/openchs-models"
    }
  },
  "dependencies": {
    "lodash": "4.17.11",
    "moment": "2.19.3",
    "rules-config": "github:openchs/rules-config#8206c29f7cc5cd2710dfa783ea28b283941ac306"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.2.0",
    "jest": "^26.6.3",
    "openchs-models": "^0.2.48"
  }
}
